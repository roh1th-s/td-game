{
	"defaults": {
		"colours": {
			"normal_bg": "#606060",
			"hovered_bg": "#707070"
		}
	},

	"tool_tip": {
		"colours": {
			"normal_text": "#FFFFFF",
			"dark_bg": "#15191eCC"
		}
	},

	"#game_title": {
		"colours": {
			"normal_text": "#c4830b",
			"dark_bg": "#FFFFFF00"
		},
		"font": {
			"name": "silkscreen",
			"size": "95",
			"regular_path": "data/fonts/Silkscreen-Regular.ttf",
			"bold_path": "data/fonts/Silkscreen-Bold.ttf"
		}
	},

	"@primary_button": {
		"colours": {
			"normal_bg": "#1C1C1C",
			"hovered_bg": "#000000",
			"hovered_text": "#c4830b",
			"active_bg": "#000000",
			"active_text": "#c4830b",
			"active_border": "#c4830b"
		},
		"font": {
			"name": "silkscreen",
			"size": "25",
			"regular_path": "data/fonts/Silkscreen-Regular.ttf"
		},
		"misc": {
			"shape": "rounded_rectangle",
			"shape_corner_radius": "10",
			"state_transitions": {
				"normal_hovered": "0.2",
				"hovered_normal": "0.2"
			}
		}
	},

	"@small_text_button": {
		"prototype": "@primary_button",
		"font": {
			"size": "20"
		}
	},

	"#wave_number": {
		"colours": {
			"normal_text": "#FFFFFF",
			"dark_bg": "#FFFFFF00"
		},
		"font": {
			"size": "40"
		}
	},
	"#health_label": {
		"colours": {
			"normal_text": "#FFFFFF",
			"dark_bg": "#FFFFFF00"
		},
		"font": {
			"size": "30"
		}
	},
	"#health_status": {
		"colours": {
			"filled_bar": "#255212"
		},
		"font": {
			"size": "30"
		},
		"misc": {
			"shape": "rounded_rectangle",
			"shape_corner_radius": "10"
		}
	},
	"#turret1_button": {
		"colours": {
			"normal_bg": "#FFFFFF00",
			"hovered_bg": "#FFFFFF00",
			"active_bg": "#FFFFFF11"
		},
		"images": {
			"normal_image": {
				"path": "data/images/turret.png"
			},
			"hovered_image": {
				"path": "data/images/turret.png"
			}
		},
		"misc": {
			"shape": "rounded_rectangle",
			"shape_corner_radius": "10"
		}
	},
	"#turret2_button": {
		"colours": {
			"normal_bg": "#FFFFFF00",
			"hovered_bg": "#FFFFFF00",
			"active_bg": "#FFFFFF11"
		},
		"images": {
			"normal_image": {
				"path": "data/images/turret2.png"
			},
			"hovered_image": {
				"path": "data/images/turret2.png"
			}
		},
		"misc": {
			"shape": "rounded_rectangle",
			"shape_corner_radius": "10"
		}
	},
	"#win_label": {
		"colours": {
			"normal_text": "#FFFFFF",
			"dark_bg": "#00000000",
			"text_shadow": "#505050"
		},
		"font": {
			"name": "silkscreen",
			"size": "95",
			"regular_path": "data/fonts/Silkscreen-Regular.ttf"
		},
		"misc": {
			"text_shadow": "1",
			"text_shadow_size": "1"
		}
	},

	"#end_screen_panel": {
		"colours": {
			"dark_bg": "#000000DC"
		}
	},
	"@transparent_panel": {
		"colours": {
			"dark_bg": "#00000000",
			"normal_border": "#00000000"
		}
	},
	"#main_menu_button": {
		"prototype": "@small_text_button"
	},
	"#quit_button": {
		"prototype": "@small_text_button",
		"colours": {
			"normal_bg": "#1C1C1C",
			"normal_text": "#FF0000",
			"hovered_bg": "#000000",
			"hovered_text": "#FF0000",
			"hovered_border": "#FF0000",
			"active_bg": "#000000",
			"active_text": "#FF0000",
			"active_border": "#FF0000"
		}
	}
}
